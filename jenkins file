node{
        stage('Compile-package') {
            def mvnHome = tool name: 'E:\\softwares\\apache-maven-3.5.2', type: 'maven'
            bat "${mvnHome}/bin/mvn compile"
        }
     
       stage('testing-stage') {
            def mvnHome = tool name: 'E:\\softwares\\apache-maven-3.5.2', type: 'maven'
            bat "${mvnHome}/bin/mvn test"
        }   
}
