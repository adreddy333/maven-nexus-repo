pipeline {

    agent any
    
    stages {
        stage('Compile stage') {
            steps {
                maven(maven : 'maven-3.5.2')
                bat "mvn clean compile" 
        }
    }

         stage('testing stage') {
             steps {
                bat "mvn test"
        }
    }

          stage('deployment stage') {
              steps {
                bat "mvn deploy"
        }
    }

  }

}

       
       
       
        
